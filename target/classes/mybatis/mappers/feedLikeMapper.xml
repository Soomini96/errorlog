<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.FeedLikeMapper">
    <select id="checkByIdAndNo" parameterType="Map" resultType="boolean">
        select * from feed_like where feed_no=#{no} and user_id=#{user_id};
    </select>
</mapper>